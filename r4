/*	cout << "輸入年"<< endl;
	int year;
	cin >> year;
	
	bool ans = false;
	
	
	if (year%4 == 0){
		if(year%100!=0 || year%400==0){
			ans = true;
		}
	}
	
	 
	
	if(ans){
	cout << "是閏年"<<endl;
	}
	else{
	cout << "不是閏年"<<endl;
	}
*/
	
cout << "輸入年"<< endl;
	int year;
	cin >> year;
	
	bool ans = true;
		
	if ((year%4 == 0) && (year%100!=0 || year%400==0)){
			ans = false;
	}
	
		
	if(!ans){
	cout << "是閏年"<<endl;
	}
	else{
	cout << "不是閏年"<<endl;
	}
	 


	return 0;
}
