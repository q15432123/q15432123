		cout << "輸入整數"<<endl;
	int a;
	cin >> a ;
	
	cout <<"輸入整數" <<endl;
	int b ;
	cin>> b; 
	
	cout <<"輸入整數" <<endl; 
	int c ;
	cin>> c ;
	
	int d = a ; 
	
		if (d<b){   //先判斷 B A  A大 不執行第一行 
		d = b;		 
		}
		if (d<c){		//再判斷 a > B  B大不執行 
		d=c ;
		}
	cout << "比較大的數是"<< d <<endl; 
	
	
	
	
	
	
}
	
  
  #include <iostream>
using namespace std;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {
	int a ,b,c;
	
	cout << "輸入A"<<endl;
	cin >> a;
	cout <<	"輸入B"<<endl; 
	cin >> b; 
	cout <<	"輸入c"<<endl; 
	cin >> c; 
	int x;
	
	x = a;
	
	if (x < b){
		x = b; 
	}
	if (x < c ){
		x = c ;
	}
	if (a==b && b==c){
		x= 0;
	}
	if (x != 0){
		cout << x <<endl;
	}else {cout << "一樣大"<<endl; 
	}
	
	
	
	

	//cout << c <<endl;
	//cout << "一樣大"<<endl; 
	return 0;
}
	
	
  
  
  
  #include <iostream>
using namespace std;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {
	
	// 4的倍數 
	// 不可以100倍數 
	// 可以是400倍數 
	cout << "輸入年"<< endl;
	int year;
	cin >> year;
	
	bool ans = false;
	
	
	if (year%4 == 0){
		if(year%100!=0){
			ans = true;
		}
		if(year%400==0){
		ans = true;
		}
	}
	
	 
	
	if(ans){
	cout << "是閏年"<<endl;
	}
	else{
	cout << "不是閏年"<<endl;
	}
	

	return 0;
}
	
	
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
