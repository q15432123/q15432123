int main(int argc, char** argv) {
	int a ,b,c;
	
	cout << "è¼¸å…¥A"<<endl;
	cin >> a;
	cout <<	"è¼¸å…¥B"<<endl; 
	cin >> b; 
	cout <<	"è¼¸å…¥c"<<endl; 
	cin >> c; 
	int x;
	
	x = a;
	
	if (x < b){
		x = b; 
	}
	if (x < c ){
		x = c ;
	}
	if (a==b && b==c){
		x= 0;
	}
	if (x != 0){
		cout << x <<endl;
	}else {cout << "ä¸€æ¨£å¤§"<<endl; 
	}
	
	
	
	

	//cout << c <<endl;
	//cout << "ä¸€æ¨£å¤§"<<endl; 
	return 0;
}
	
	
  
  
  
  #include <iostream>
using namespace std;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {
	
	// 4çš„å€æ•¸ 
	// ä¸å¯ä»¥100å€æ•¸ 
	// å¯ä»¥æ˜¯400å€æ•¸ 
	cout << "è¼¸å…¥å¹´"<< endl;
	int year;
	cin >> year;
	
	bool ans = false;
	
	
	if (year%4 == 0){
		if(year%100!=0){
			ans = true;
		}
		if(year%400==0){
		ans = true;
		}
	}
	
	 
	
	if(ans){
	cout << "æ˜¯é–å¹´"<<endl;
	}
	else{
	cout << "ä¸æ˜¯é–å¹´"<<endl;
	}
	

	return 0;
}
	
	
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
