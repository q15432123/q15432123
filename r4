#include <iostream>
#include <cmath> 
using namespace std; 
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {
	cout << "輸入一個數" << endl;
	int number;
	cin >> number; 
	
	double answer = sqrt(number);
	cout <<number<<"的平方根為" <<answer <<endl;
	
	if (sqrt(number)>10 ){
		cout << "他的值大於10"<<endl; 
}
	return 0;
}




#include <iostream>
using namespace std;

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {
	int  a =3*3*3.14159 ;
	cout << a << endl;
	
	double b=123.456;
	int c = 123*456*b;
	int d = 123*456*(int)b;
	int e = (int)(123*456*b);
	
	cout << b<<" " << endl;
	cout << c<< " " <<d <<endl;
	cout<< e << endl;

	
	
	return 0;
}




#include <iostream>
using namespace std;

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {
	
	int a ,b ;
	
	cout << "輸入整數A"<< endl;
	cin >> a ; 
	cout <<	"輸入整數B"<< endl; 
	cin >> b ;
	if (a>b) {
		cout <<"a大 a是" << a<<endl; 
	}
	if (b>a){				//else {}
		cout << "b大 b是"<<b <<endl;
	}
	if (a==b){
		cout << "a跟b一樣大"<< endl; 
	}
	
	
	return 0;
}










