int main(int argc, char** argv) {
	std::cout	<< 8 			<<std::endl;
	std::cout	<< 3.14			<<std::endl;
	std::cout	<<'a'			<<std::endl;
	std::cout	<< "Hello World"<<std::endl;
	std::cout	<< true			<<std::endl;
	std::cout	<< false		<<std::endl; 
	return 0;
}


#include <iostream>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {
	int a ;
	int b = 10;
	int c, d =20;
	std::cout << a <<std::endl;
	std::cout << b <<std::endl;
	std::cout << c << std::endl;
	std::cout << d << std::endl;
 	a= 10;
 	b= 20;
 	c= a;
 	d= b;
 	std::cout << a <<std::endl;
	std::cout << b <<std::endl;
	std::cout << c << std::endl;
	std::cout << d << std::endl;
 	
	const double pi = 3.14159;
	std::cout << pi << std::endl;
	return 0;
}
