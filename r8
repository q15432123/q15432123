### Hi there 👋
int main(int argc, char** argv) {
	int count = 0;
	char c = 'y';
	while (c!= 'n'&& c != 'N'){
		cout << "請輸入一個年份" <<endl;
		int year = 0 ;
		cin >> year ;
		
		
		
		bool answer = true;
	
		if (year %4 == 0 && year %100 != 0 || year %400 ==0 ){
			answer = true;
			count ++;
		}
		if (answer){
			cout << "閏年" <<endl;   
		}else {
			cout << "平年" <<endl; 
		}
	cout <<"請輸入N/n結束,其他鍵繼續執行" <<endl;
	cin >> c;
	
	}
	cout << count <<endl;	
 	
	return 0;
	
}


#include <iostream>
#include <string>
using namespace std; 

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {

	//
	
	int a =0,n=4 ; 
	
	
	
	while (n!=5){
		cout <<"1提款 2轉帳 3餘額查詢 4其他服務 5離開"<<endl;
		cin >> n ;
		bool b = true ;
	switch (n){
			case 1:
				cout <<"提款中" <<endl;
				break;
			case 2:
				cout <<"轉帳中" <<endl;
				break;
			case 3:
				cout <<"餘額 為0" <<endl;
				break;
			case 4:
				cout <<"沒有服務" <<endl;
				break;
			case 5:
				cout <<"離開" <<endl;
				break;
			default:
				cout <<"請輸入正確選項" <<endl;
				break; 
		}if (!b){
			break;
		}
	}
		
	
 	
	return 0;
	
}
