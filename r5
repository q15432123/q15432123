	int score;
	cout << "輸入此次成績" << endl;
	cin >>score ;

	string a ="丁" ;
	
	if(score >100|| score < 0){
		a = "輸入錯誤";
	}else if(score>= 90){
			a="優"; 
	}else if (score >=80){
			a = "甲";
	}else if (score >=70){
			a = "乙";
	}else if (score >=60){
			a = "丙";
	}
		
	cout << a << endl;
	
	return 0;
}
