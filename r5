	int month ;
	cout << "月份"<<endl;
	cin >> month;
	
	int days= 31;
	switch(month){	
			days = 31;
			break;
		case 2:
			days = 28;
			break;
		case 4:
		case 6:
		case 9:
		case 11:
			days = 30;
			break;			
		default:
			if (month < 1 || month >12){
			days= -1 ;}
	}
	if (days != -1 ){
		cout << days << endl;
		}else{cout << "輸入錯誤" <<endl;}
	
	system ("pause");



	return 0;
}









